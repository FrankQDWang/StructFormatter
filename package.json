{
  "name": "structformatter",
  "version": "0.1.2",
  "description": "OpenAI-compatible proxy that enforces JSON Schema structured outputs for upstream LLM APIs that do not support native constrained decoding.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrankQDWang/StructFormatter.git"
  },
  "homepage": "https://github.com/FrankQDWang/StructFormatter#readme",
  "bugs": {
    "url": "https://github.com/FrankQDWang/StructFormatter/issues"
  },
  "keywords": [
    "openai",
    "proxy",
    "structured-outputs",
    "json-schema",
    "ajv",
    "fastify"
  ],
  "engines": {
    "node": ">=20"
  },
  "bin": {
    "structformatter": "dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "config.example.yaml",
    "CHANGELOG.md",
    "README.md",
    "README.zh-CN.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/cli.js",
    "lint": "eslint .",
    "format": "prettier -w .",
    "prepack": "npm run build",
    "test": "vitest run"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "fastify": "^5.6.2",
    "jsonrepair": "^3.13.1",
    "undici": "^7.16.0",
    "yaml": "^2.8.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
